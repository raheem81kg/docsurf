{
   "$schema": "https://openapi.vercel.sh/vercel.json",
   "buildCommand": "pnpm turbo build --filter=web -- --ignore-scripts",
   "headers": [
      {
         "source": "/assets/(.*)",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000, immutable"
            }
         ]
      }
   ]
}
