{
   "$schema": "https://turbo.build/schema.json",
   "ui": "tui",
   "tasks": {
      "build": {
         "dependsOn": ["^build"],
         "inputs": ["$TURBO_DEFAULT$", ".env*"],
         "outputs": ["dist/**", ".output/**", "vinxi/**", ".vercel/**", ".tanstack/**"]
      },
      "lint": {
         "dependsOn": ["^lint"]
      },
      "check-types": {
         "dependsOn": ["^check-types"]
      },
      "dev": {
         "cache": false,
         "persistent": true
      },
      "setup": {
         "cache": false,
         "persistent": true
      }
   }
}
