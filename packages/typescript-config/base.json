{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Base",
	"compilerOptions": {
		"types": ["node"],
		"strict": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"rootDirs": ["."]
	}
}
